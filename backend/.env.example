NODE_ENV=development
PORT=3000

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/databasename

# JWT
JWT_SECRET=your-secret-key-change-in-production-256-bits
JWT_REFRESH_SECRET=your-refresh-secret-key-change-in-production-256-bits
JWT_ACCESS_TOKEN_EXPIRES_IN=30m
JWT_REFRESH_TOKEN_EXPIRES_IN=7d

# Frontend
FRONTEND_URL=http://localhost:5173

# Redis (for Bull Queue)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_USERNAME=default
REDIS_PASSWORD=your-password

# Rate Limiting (optional - defaults in parentheses)
RATE_LIMIT_FEEDBACK_DAILY_LIMIT=2
